{
  "env": {
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": [
        "transform-object-rest-spread",
        "transform-react-jsx",
        "transform-class-properties"
      ]
    },
    "development": {
      "presets": [
        [ "@babel/preset-env", { "modules": false } ],
        [ "@babel/preset-react", { "modules": false } ]
      ],
      "plugins": [
        "transform-object-rest-spread",
        "transform-react-jsx",
        "transform-class-properties",
      ]
    },
    "production": {
      "presets": [
        [ "@babel/preset-env", { "modules": false } ],
        [ "@babel/preset-react", { "modules": false } ]
      ],
      "plugins": [
        "transform-object-rest-spread",
        "transform-react-jsx",
        "transform-class-properties",
      ]
    }
  } 
}
